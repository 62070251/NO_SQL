<head>
    <script type="text/javascript" src="scripts.js"></script>
    <style>
        ul {
            list-style-type: none;
            margin: 0;
            padding: 0;
            width: 250px;
            background-color: #ffffff;
            opacity: 0.5;
            border-radius: 2px;
        }

        li {
            text-align: center;
            border-bottom: 1px solid #8FCAF1;
        }

        li:last-child {
            border-bottom: none;
        }

        li a {
            display: block;
            color: black;
            padding: 10px;
            text-decoration: none;
            font-size: 20px;
        }

        /* When the link is hovered on, its color will change */
        li a:hover {
            background-color: #BDCDD7;
            color: black;
        }
    </style>
</head>

